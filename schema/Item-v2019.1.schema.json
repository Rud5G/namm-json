{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Item v2019.1",
    "description": "NAMM Item v2019.1",
    "additionalProperties": false,
    "properties": {
        "version": {
            "type": "number"
        },
        "Timestamp": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "SupplierId": {
            "type": "string"
        },
        "Items": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "#/definitions/Item"
            }
        }
    },
    "definitions": {
        "Item": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "SupplierItemId": {
                    "type": "string"
                },
                "SupplierModel": {
                    "type": "string"
                },
                "SupplierItemDesc": {
                    "type": "string"
                },
                "Kit": {
                    "type": "string",
                    "enum": [
                        "Y",
                        "N"
                    ]
                },
                "KitComponentOnly": {
                    "type": "string",
                    "enum": [
                        "Y",
                        "N"
                    ]
                },
                "Serialized": {
                    "type": "string",
                    "enum": [
                        "Y",
                        "N"
                    ]
                },
                "BrandName": {
                    "type": "string"
                },
                "CountryOrigin": {
                    "type": "string",
                    "maxLength": 2,
                    "minLength": 2
                },
                "Length": {
                    "type": "number"
                },
                "Width": {
                    "type": "number"
                },
                "Height": {
                    "type": "number"
                },
                "DimUOM": {
                    "$ref": "CommonSchema.json#/definitions/DimUOM"
                },
                "Weight": {
                    "type": "number"
                },
                "WeightUOM": {
                    "$ref": "CommonSchema.json#/definitions/WeightUOM"
                },
                "AnnounceDate": {
                    "type": "string",
                    "format": "date"
                },
                "AvailableDate": {
                    "type": "string",
                    "format": "date"
                },
                "UnavailableDate": {
                    "type": "string",
                    "format": "date"
                },
                "EndOfProductionDate": {
                    "type": "string",
                    "format": "date"
                },
                "ReplacementID": {
                    "type": "string"
                },
                "WarrantyPartTerm": {
                    "type": "integer",
                    "maximum": 65535,
                    "minimum": 0
                },
                "WarrantyPartUOM": {
                    "$ref": "CommonSchema.json#/definitions/TermUOM"
                },
                "WarrantyLabTerm": {
                    "type": "integer",
                    "maximum": 255,
                    "minimum": 0
                },
                "WarrantyLabUOM": {
                    "$ref": "CommonSchema.json#/definitions/TermUOM"
                },
                "Condition": {
                    "type": "string"
                },
                "Barcodes": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Barcode"
                    },
                    "required": []
                },
                "Pricing": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Value": {
                                "type": "number"
                            },
                            "type": {
                                "type": "string",
                                "enum": [
                                    "MSRP",
                                    "MAP",
                                    "Cost"
                                ]
                            },
                            "Currency": {
                                "$ref": "CommonSchema.json#/definitions/CurrencyCode"
                            },
                            "Qty": {
                                "type": "integer",
                                "maximum": 255,
                                "minimum": 0
                            }
                        },
                        "required": [
                            "type",
                            "Currency"
                        ]
                    }
                },
                "Containers": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "Type": {
                                "type": "string"
                            },
                            "Length": {
                                "type": "number"
                            },
                            "Width": {
                                "type": "number"
                            },
                            "Height": {
                                "type": "number"
                            },
                            "DimUOM": {
                                "$ref": "CommonSchema.json#/definitions/DimUOM"
                            },
                            "Weight": {
                                "type": "number"
                            },
                            "WeightUOM": {
                                "$ref": "CommonSchema.json#/definitions/WeightUOM"
                            },
                            "UOM": {
                                "$ref": "CommonSchema.json#/definitions/QtyUOM"
                            },
                            "QtyPerUOM": {
                                "type": "integer",
                                "maximum": 65535,
                                "minimum": 0
                            },
                            "Barcodes": {
                                "type": "object",
                                "additionalProperties": false,
                                "properties": {
                                    "Barcode": {
                                        "type": "array",
                                        "minItems": 1,
                                        "items": {
                                            "type": "object",
                                            "additionalProperties": false,
                                            "properties": {
                                                "ID": {
                                                    "type": "string"
                                                },
                                                "Type": {
                                                    "$ref": "CommonSchema.json#/definitions/BarCodeType"
                                                }
                                            },
                                            "required": [
                                                "Type"
                                            ]
                                        }
                                    }
                                },
                                "required": []
                            }
                        },
                        "required": [
                            "Type",
                            "Length",
                            "Width",
                            "Height",
                            "DimUOM",
                            "Weight",
                            "WeightUOM",
                            "UOM",
                            "QtyPerUOM"
                        ]
                    }
                },
                "KitItems": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "SupplierItemId": {
                                "type": "string"
                            },
                            "Qty": {
                                "type": "number"
                            },
                            "UOM": {
                                "$ref": "CommonSchema.json#/definitions/QtyUOM"
                            },
                            "Barcodes": {
                                "type": "array",
                                "minItems": 1,
                                "items": {
                                    "$ref": "#/definitions/Barcode"
                                }
                            }
                        },
                        "required": [
                            "SupplierItemId",
                            "Qty",
                            "UOM"
                        ]
                    }
                },
                "MarketingInfo": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "PrimaryCategory": {
                            "type": "string"
                        },
                        "ProductURL": {
                            "type": "string",
                            "format": "uri"
                        },
                        "ItemDescLong": {
                            "type": "string"
                        },
                        "SecondaryCategories": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "Bullets": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "Media": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "Images": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/Image"
                                    }
                                },
                                "Videos": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/Video"
                                    }
                                },
                                "Documents": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/Document"
                                    }
                                },
                                "Audios": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/Audio"
                                    }
                                }
                            }
                        },
                        "Keywords": {
                            "type": "array",
                            "minItems": 1,
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "required": [
                        "PrimaryCategory"
                    ]
                },
                "ExtendedAttributes": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ExtendedAttribute"
                    }
                },
                "RelatedItems": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/RelatedItem"
                    }
                },
                "ItemFamily": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "property": {
                            "type": "string"
                        },
                        "code": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "property",
                        "code"
                    ]
                },
                "Availability": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "QtyOnHand": {
                            "type": "integer",
                            "maximum": 255,
                            "minimum": 0
                        },
                        "Available": {
                            "type": "string",
                            "enum": [
                                "YES",
                                "NO",
                                "LOW",
                                "MEDIUM",
                                "HIGH"
                            ]
                        },
                        "QueryURL": {
                            "type": "string",
                            "format": "uri"
                        }
                    },
                    "required": [
                        "Available"
                    ]
                },
                "Prop65Message": {
                    "type": "string"
                },
                "Prop65Chemical": {
                    "type": "string"
                }
            },
            "required": []
        },
        "Barcode": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ID": {
                    "type": "string"
                },
                "Type": {
                    "$ref": "CommonSchema.json#/definitions/BarCodeType"
                }
            },
            "required": []
        },
        "RelatedItem": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "type": {
                    "type": "string",
                    "enum": [
                        "similar",
                        "accessory",
                        "ThirdParty"
                    ]
                },
                "included": {
                    "type": "string",
                    "enum": [
                        "Y",
                        "N"
                    ]
                },
                "SupplierItemId": {
                    "type": "string"
                },
                "Barcode": {
                    "$ref": "#/definitions/Barcode"
                }
            }
        },
        "Image": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "URL": {
                    "type": "string",
                    "format": "uri"
                },
                "Audience": {
                    "type": "string",
                    "enum": [
                        "Consumer",
                        "Dealer"
                    ]
                },
                "View": {
                    "type": "string",
                    "enum": [
                        "Front cover",
                        "Preface",
                        "Back cover",
                        "Contents",
                        "Primary"
                    ]
                },
                "Caption": {
                    "type": "string"
                },
                "CarouselSequence": {
                    "type": "integer"
                }
            }
        },
        "Video": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "URL": {
                    "type": "string",
                    "format": "uri"
                },
                "Audience": {
                    "type": "string"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "flv",
                        "mp4",
                        "mov",
                        "avi"
                    ]
                },
                "Caption": {
                    "type": "string"
                }
            }
        },
        "Document": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "URL": {
                    "type": "string",
                    "format": "uri"
                },
                "Audience": {
                    "type": "string"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "pdf",
                        "doc",
                        "docx",
                        "txt"
                    ]
                },
                "Caption": {
                    "type": "string"
                }
            }
        },
        "Audio": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "URL": {
                    "type": "string",
                    "format": "uri"
                },
                "Audience": {
                    "type": "string",
                    "enum": [
                        "Consumer"
                    ]
                },
                "type": {
                    "type": "string"
                },
                "Caption": {
                    "type": "string"
                }
            }
        },
        "ExtendedAttribute": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Value": {
                    "additionalProperties": false,
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Contributor"
                        },
                        {
                            "type": "string",
                            "title": "Extended Attribute Name",
                            "description": "This might have enums?"
                        },
                        {
                            "type": "number"
                        }
                    ]
                }
            }
        },
        "Contributor": {
            "type": "object",
            "properties": {
                "Primary": {
                    "type": "boolean"
                },
                "Contribution": {
                    "type": "string",
                    "enum": [
                        "Author",
                        "Artist",
                        "Composer",
                        "Arranger"
                    ]
                },
                "ContributorName": {
                    "type": "string"
                },
                "ContributorNameReversed": {
                    "type": "string"
                }
            }
        }
    }
}