{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "NAMM_FunctionalAcknowledgement",
    "additionalProperties": false,
    "properties": {
        "version": {
            "type": "number"
        },
        "Timestamp": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Supplier": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PartyId": {
                    "type": "string",
                    "maxLength": 25
                }
            },
            "required": [
                "PartyId"
            ]
        },
        "SoldTo": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "PartyId": {
                    "type": "string",
                    "maxLength": 25
                }
            },
            "required": [
                "PartyId"
            ]
        },
        "StatusCode": {
            "type": "string",
            "enum": [
                "C01",
                "C02"
            ]
        },
        "StatusMessage": {
            "type": "string",
            "maxLength": 10
        }
    },
    "required": []
}